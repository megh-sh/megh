{
    "name": "megh/megh-cli",
    "description": "Megh CLI",
    "keywords": [
        "docker",
        "php",
        "server",
        "ubuntu",
        "nginx",
        "mysql",
        "mariadb"
    ],
    "homepage": "https://megh.sh",
    "license": "MIT",
    "authors": [
        {
            "name": "Tareq Hasan",
            "email": "tareq@hey.com"
        }
    ],
    "require": {
        "illuminate/collections": "^9.0@dev",
        "illuminate/container": "^9.0@dev",
        "symfony/console": "^5.2",
        "symfony/process": "5.x-dev",
        "symfony/yaml": "5.x-dev",
        "vlucas/phpdotenv": "^5.2@dev"
    },
    "autoload": {
        "psr-4": {
            "Megh\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev"
}
