{
    "name": "tareq1988/pocker",
    "description": "A docker PHP development server",
    "type": "project",
    "authors": [
        {
            "name": "Tareq Hasan",
            "email": "tareq@wedevs.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.2"
        }
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Megh\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2",
        "mnapoli/silly": "1.7",
        "symfony/process": "^4.0",
        "symfony/yaml": "4.4.x-dev",
        "vlucas/phpdotenv": "^5.2@dev"
    },
    "minimum-stability": "dev"
}
